---
import { Previous } from "paully/svgs";
---

<button
  class="previous"
  title="previous projects page"
  style={{ visibility: "hidden" }}
>
  <Previous className="any" height="16" />
</button>

<style lang="scss">
  @use "~mixins" as *;

  .previous {
    @include trello-button;

    svg {
      @include medium-up {
        left: -1px;
      }

      @include medium-down {
        top: -1px;
      }
    }
  }
</style>
