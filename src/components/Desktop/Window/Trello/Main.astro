---
import { projects } from "~data/layout.json";
import Card from "~components/Desktop/Window/Trello/Card.astro";
---

<main>
  {
    projects.map(function (project, index) {
      const props = project;

      if (index >= 2) props.style = { display: "none" };

      return <Card {...props} />;
    })
  }
</main>
