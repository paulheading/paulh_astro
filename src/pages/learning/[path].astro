---
import Layout from "~layouts/Page.astro";

import { learning } from "~data/resume.json";
import { get } from "~scripts/helpers";

export function getStaticPaths() {
  return get.StaticPaths(learning);
}

const { name, local } = Astro.props;

const back = {
  href: "/resume",
  title: "Resume",
};

const layout = {
  title: "Paul Heading | Learning | " + name,
  description: local.summary,
};

const props = {
  back,
  layout,
};
---

<Layout {...props} />
