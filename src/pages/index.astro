---
import Layout from "~layouts/Main.astro";
import Hero from "~components/Hero.astro";

import { data } from "paully";

let { seo } = data;

// import { pocketcasts } from "~data/layout.json";

const layoutProps = {
  keywords: seo.keywords("portfolio"),
  description: "I'm a " + seo.job.title + ". " + seo.job.description,
};
---

<Layout {...layoutProps}>
  <Hero />
</Layout>
